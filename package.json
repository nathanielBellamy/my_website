{
  "name": "my_website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cd src-rust && wasm-pack && wasm-bindgen ./target/wasm32-unknown-unknown/release/src_rust.wasm --target=no-modules --out-dir=pkg && cd .. && vite",
    "build": "cd src-rust & cargo fmt --all & wasm-pack build --target web & cd .. && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-wasm": "^6.1.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^18.15.11",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "node-sass": "^8.0.0",
    "svelte-watch-resize": "^1.0.3"
  }
}
